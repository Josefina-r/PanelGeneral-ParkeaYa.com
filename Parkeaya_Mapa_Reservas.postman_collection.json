{
  "info": {
    "name": "Parkeaya - Mapa y Reservas",
    "description": "Endpoints para obtener estacionamientos en mapa y crear/gestionar reservas",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "üó∫Ô∏è MAPA - Obtener Estacionamientos",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{base_url}}/parking/parkings/?available=true&aprobado=true&activo=true",
          "host": ["{{base_url}}"],
          "path": ["parking", "parkings"],
          "query": [
            {
              "key": "available",
              "value": "true"
            },
            {
              "key": "aprobado",
              "value": "true"
            },
            {
              "key": "activo",
              "value": "true"
            }
          ]
        }
      },
      "response": []
    },
    {
      "name": "üó∫Ô∏è MAPA - Filtrar por Seguridad",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{base_url}}/parking/parkings/?nivel_seguridad=alta&available=true",
          "host": ["{{base_url}}"],
          "path": ["parking", "parkings"],
          "query": [
            {
              "key": "nivel_seguridad",
              "value": "alta"
            },
            {
              "key": "available",
              "value": "true"
            }
          ]
        }
      },
      "response": []
    },
    {
