        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{access_token}}"
          }
        ],
        "url": {
          "raw": "{{base_url}}/reservations/reservations/1/",
          "host": ["{{base_url}}"],
          "path": ["reservations", "reservations", "1"]
        }
      },
      "response": []
