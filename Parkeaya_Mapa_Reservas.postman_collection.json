        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{base_url}}/parking/parkings/1/",
          "host": ["{{base_url}}"],
          "path": ["parking", "parkings", "1"]
        }
      },
      "response": []
    },
